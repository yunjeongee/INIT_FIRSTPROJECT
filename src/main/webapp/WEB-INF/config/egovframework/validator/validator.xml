<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
        <form name="sampleVO">
         
          <field property="goodsNum" depends="required">
              <arg0 key="title.sample.goodsNum" />
		  </field>
		  		  
		  
		  <field property="manufactureName" depends="required">
              <arg0 key="title.sample.manufactureName" />
		  </field>
		  
		  <field property="goodsName" depends="required">
              <arg0 key="title.sample.goodsName" />
		  </field>		  		  

		   <field property="material" depends="required">
              <arg0 key="title.sample.material" />
		  </field>			  

		   <field property="goodsFuntion" depends="required">
              <arg0 key="title.sample.goodsFuntion" />
		  </field>				  
		  		 
		  <field property="imgUrl" depends="required">
              <arg0 key="title.sample.imgUrl" />
		  </field>



        </form>
    </formset>

</form-validation>